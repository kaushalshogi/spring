	<bean id="wallet" class="com.workz.wallet.Wallet" scope="prototype" >
	<constructor-arg name="brand" value="puma"></constructor-arg>
	<constructor-arg name="color" value="white"></constructor-arg>
	<constructor-arg name="price" value="3000"></constructor-arg>
	<constructor-arg name="material" ref="material"></constructor-arg>
	<constructor-arg name="money" ref="money"></constructor-arg>
	<constructor-arg name="atmcard" ref="atmcard"></constructor-arg>
	<constructor-arg name="photo" ref="photo"></constructor-arg>
	</bean>
	
	<bean class="com.workz.wallet.Material" id="material" >
	<constructor-arg name="brand" value="synthetic"></constructor-arg>
	<constructor-arg name="color" value="red"></constructor-arg>
	<constructor-arg name="price" value="300"></constructor-arg></bean>
	
	<bean class="com.workz.wallet.Money" id="money"></bean>

	<bean class="com.workz.wallet.ATMCard" id="atmcard"></bean>

	<bean class="com.workz.wallet.Photo" id="photo"></bean>




</beans>