<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean
		class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
		<property name="location" value="photo.properties"></property>
	</bean>

	<bean class="com.workz.wallet.Photo" id="photo">
		<constructor-arg name="pPrize" value="${price}"></constructor-arg>
		<constructor-arg name="pSize" value="${photoSize}"></constructor-arg>
	</bean>

