<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">
          

			<bean class="com.xworkz.lockapp.Key" id="key">
			<property name="brand" value="${brand}"> </property>
			<property name="size" value="${size}"></property>
			<property name="length" value="${length}"></property>
			<property name="weight" value="${weight}"></property>
			</bean> 
			<bean class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
			<property name="location" value="key.properties"></property>
			
			</bean>
			
			
			          
           </beans>